{
  "name": "kickass-search",
  "description": "Simple search API for Kickass Torrents",
  "version": "0.0.4",
  "dependencies": {
    "q": "^1.1.2",
    "request": "^2.51.0"
  },
  "scripts": {
    "lint": "jshint .",
    "checkStyle": "jscs .",
    "pretest": "npm run-script lint && npm run-script checkStyle",
